{
  "cli": {
    "version": ">= 0.46.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development",
        "FIREBASE_API_KEY": "public",
        "FIREBASE_PROJECT_ID": "public",
        "FIREBASE_MESSAGING_SENDER_ID": "public",
        "FIREBASE_APP_ID": "public"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development",
        "FIREBASE_API_KEY": "public",
        "FIREBASE_PROJECT_ID": "public",
        "FIREBASE_MESSAGING_SENDER_ID": "public",
        "FIREBASE_APP_ID": "public"
      }
    },
    "production": {
      "releaseChannel": "production",
      "distribution": "store",
      "env": {
        "APP_ENV": "production",
        "FIREBASE_API_KEY": "public",
        "FIREBASE_PROJECT_ID": "public",
        "FIREBASE_MESSAGING_SENDER_ID": "public",
        "FIREBASE_APP_ID": "public"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
